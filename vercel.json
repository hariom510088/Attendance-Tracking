{
    "version": 2,
    "builds": [
        {
            "src": "ATTENDANCE/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/ATTENDANCE/wsgi.py"
        }
    ]
}